{
  "name": "downshiftorg/wp-rest-router",
  "type": "library",
  "authors": [
    {
      "name": "Josh Arens",
      "email": "josh@pro.photo"
    }
  ],
  "autoload": {
    "psr-4": {
      "Downshift\\": "./src"
    }
  },
  "scripts": {
    "analyse": "phpstan analyse src",
    "analyse:fix-stubs": "PHPStan\\WordPress\\Composer\\FixWpStubs::php73Polyfill",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "lint": "phpcs",
    "test:unit": "kahlan --config=test/unit/kahlan.php --spec=test/unit --reporter=verbose",
    "test:int": "kahlan --config=test/integration/kahlan.php --spec=test/integration/spec --reporter=verbose",
    "test": "composer test:unit && composer test:int"
  },
  "require-dev": {
    "kahlan/kahlan": "^4.6",
    "squizlabs/php_codesniffer": "^3.4",
    "phpstan/phpstan": "^0.11.15",
    "szepeviktor/phpstan-wordpress": "^0.2.0",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require": {
  }
}
